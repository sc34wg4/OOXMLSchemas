<?xml version="1.0" encoding="UTF-8"?>
<root xmlns:mce="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:foo="http://www.example.com/foo" xmlns:bar="http://www.example.com/bar"
    mce:ProcessContent="foo:unwrapped">
    <mce:AlternateContent>
        <!-- Choice #1 -->
        <mce:Choice Requires="foo">
            <!-- Foo #1 -->
            <foo:foo/>
            <!-- Bar #1 -->
            <bar:bar>
                <mce:AlternateContent>
                    <!-- Choice #1-1 -->
                    <mce:Choice Requires="bar">Choice1-1-Content</mce:Choice>
                    <!-- Fallback #1-1 -->
                    <mce:fallback>Fallback1-Content</mce:fallback>
                </mce:AlternateContent>
            </bar:bar>
        </mce:Choice>
        <!-- Choice #2 -->
        <mce:Choice Requires="bar">
            <!-- Bar #2 -->
            <bar:bar/>
            <foo:unwrapped>
                <mce:AlternateContent>
                    <!-- Choice #2-1 -->
                    <mce:Choice Requires="foo">Choice2-1-Content</mce:Choice>
                    <!-- Fallback #2-1 -->
                    <mce:fallback>Fallback2-Content</mce:fallback>
                </mce:AlternateContent>
            </foo:unwrapped>
        </mce:Choice>
    </mce:AlternateContent>
</root>
